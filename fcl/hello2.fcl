#
# The first example of running art: how to run one module.
#
#include "minimalMessageService.fcl"
services.message: @local::default_message

process_name: helloWorld2

source: {
  module_type: EmptyEvent
  maxEvents:  2
}

physics: {
  analyzers: {
    hello2: {
            module_type: HelloWorld2
            magicNumber: 42
    }
  }

  path1: [ hello2 ]
    end_paths: [ path1 ]

}
